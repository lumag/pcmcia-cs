#
# Memory Technology Drivers
#
region "Generic or SRAM"
  default
  mtd "sram_mtd"

region "Intel Series 2 Flash"
  jedec 0x89 0xa2
  mtd "iflash2_mtd"

region "Intel Series 100 Flash"
  jedec 0x89 0xaa
  mtd "iflash2_mtd"

region "Intel Series 2+ Flash"
  jedec 0x89 0xa0
  mtd "iflash2+_mtd"

